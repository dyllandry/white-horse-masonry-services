doctype html
html
head
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1')
  link(href='styles/index.css' rel=`stylesheet`)
  title White Horse Masonry
body
header
  .header--bg
  .width-limiter
    .image-strips
      .image-strips--wide
        img(src="images/wide_1.jpg")
        img(src="images/wide_2.jpg")
        img(src="images/wide_3.jpg")
      .image-strips--skinny
        img(src="images/skinny_1.jpg")
        img(src="images/skinny_2.jpeg")
        img(src="images/skinny_3.jpg")
    .titles
      .titles--first
        h1 MASTERWORK
        h1 MASONRY
      .titles--second
        h1 WHITE HORSE
        h1 MASONRY
article.content
  .width-limiter
    .free-estimate
      .free-estimate--title Free Estimate
      .free-estimate--body
        .free-estimate--form
          input(type=`text`)
          input(type=`email`)
          textarea
